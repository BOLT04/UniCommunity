<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent" android:id="@+id/linearLayout5">
    <fragment
            android:name="androidx.navigation.fragment.NavHostFragment"
            android:layout_width="399dp"
            android:layout_height="304dp" app:navGraph="@navigation/nav_graph_testing" app:defaultNavHost="true"
            android:id="@+id/nav_host" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <Button
            android:text="A"
            android:layout_width="89dp"
            android:layout_height="38dp"
            android:id="@+id/A" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/nav_host" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <Button
            android:text="B"
            android:layout_width="89dp"
            android:layout_height="38dp"
            android:id="@+id/B" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/A" app:layout_constraintStart_toStartOf="@+id/A"
    />
    <Button
            android:text="C"
            android:layout_width="89dp"
            android:layout_height="38dp"
            android:id="@+id/C" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/B" app:layout_constraintStart_toStartOf="@+id/B"
    />
</androidx.constraintlayout.widget.ConstraintLayout>